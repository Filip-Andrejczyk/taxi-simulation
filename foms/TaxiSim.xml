<?xml version='1.0' encoding='utf-8'?>
<objectModel
        xmlns='http://standards.ieee.org/IEEE1516-2010'
        xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
        xsi:schemaLocation='http://standards.ieee.org/IEEE1516-2010 http://standards.ieee.org/downloads/1516/1516.2-2010/IEEE1516-DIF-2010.xsd'  >
    <modelIdentification>
        <name>TaxiSim</name>
        <type>FOM</type>
        <version>1.0</version>
    </modelIdentification>
    <objects>
        <objectClass>
            <name>HLAobjectRoot</name>
            <objectClass>
                <name>Taxis</name>
                <sharing>PublishSubscribe</sharing>
                <semantics>Taxis dokonujaca przewozu klienta</semantics>
                <attribute>
                    <name>areaId</name>
                    <dataType>HLAinteger32BE</dataType>
                    <updateType>Static</updateType>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>HLAreliable</transportation>
                    <order>TimeStamp</order>
                </attribute>
                <attribute>
                    <name>taxiId</name>
                    <dataType>HLAinteger32BE</dataType>
                    <updateType>Static</updateType>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>HLAreliable</transportation>
                    <order>TimeStamp</order>
                </attribute>
            </objectClass>
            <objectClass>
                <name>Passengers</name>
                <sharing>PublishSubscribe</sharing>
                <semantics>Pasazer kozystajacy z uslug taxi</semantics>
                <attribute>
                    <name>passengerId</name>
                    <dataType>HLAinteger32BE</dataType>
                    <updateType>Static</updateType>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>HLAreliable</transportation>
                    <order>TimeStamp</order>
                </attribute>
                <attribute>
                    <name>originId</name>
                    <dataType>HLAinteger32BE</dataType>
                    <updateType>Static</updateType>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>HLAreliable</transportation>
                    <order>TimeStamp</order>
                </attribute>
                <attribute>
                    <name>directionId</name>
                    <dataType>HLAinteger32BE</dataType>
                    <updateType>Static</updateType>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>HLAreliable</transportation>
                    <order>TimeStamp</order>
                </attribute>
            </objectClass>
        </objectClass>
    </objects>
    <interactions>
        <interactionClass>
            <name>HLAinteractionRoot</name>
            <interactionClass>
                <name>executeRide</name>
                <parameter>
                    <name>destinationId</name>
                    <dataType>HLAinteger32BE</dataType>
                </parameter>
                <parameter>
                    <name>passengerId</name>
                    <dataType>HLAinteger32BE</dataType>
                </parameter>
                <parameter>
                    <name>taxiId</name>
                    <dataType>HLAinteger32BE</dataType>
                </parameter>
                <order>TimeStamp</order>
            </interactionClass>
            <interactionClass>
                <name>publishNumOfAreas</name>
                <parameter>
                    <name>numOfAreas</name>
                    <dataType>HLAinteger32BE</dataType>
                </parameter>
                <order>TimeStamp</order>
            </interactionClass>
        </interactionClass>
    </interactions>
    <dimensions/>
    <synchronizations>
        <synchronizationPoint>
            <label>ReadyRoRun</label>
            <dataType>NA</dataType>
            <capability>NA</capability>
            <semantics>Waiting for each federate joining</semantics>
        </synchronizationPoint>
    </synchronizations>
    <transportations/>
    <updateRates/>
    <dataTypes>
        <basicDataRepresentations/>
        <simpleDataTypes/>
        <enumeratedDataTypes/>
        <arrayDataTypes/>
        <fixedRecordDataTypes/>
        <variantRecordDataTypes/>
    </dataTypes>
    <notes/>
</objectModel>
